{
  "name": "noble-secp256k1",
  "version": "1.0.5",
  "description": "Noble secp256k1. Very fast, high-security, auditable, 0-dep, 1-file pubkey & ECDSA.",
  "main": "dist",
  "scripts": {
    "build": "tsc -d",
    "lint": "prettier --print-width 100 --single-quote --check index.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "bench": "node test/benchmark.js"
  },
  "jest": {
    "testRegex": "/test/.*?\\.ts",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "author": "Paul Miller (https://paulmillr.com)",
  "homepage": "https://paulmillr.com/posts/noble-secp256k1-fast-ecc/",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulmillr/noble-secp256k1.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.36",
    "babel-plugin-transform-bigint": "^1.0.7",
    "fast-check": "^1.24.1",
    "jest": "^25.4.0",
    "micro-bmark": "^0.1.2",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "typescript": "3.8.3"
  },
  "keywords": [
    "secp256k1",
    "secp",
    "secp256",
    "elliptic",
    "elliptic curve",
    "curve",
    "signature",
    "ecc",
    "rfc6979",
    "ecdsa",
    "endomorphism",
    "cryptography",
    "security",
    "noble"
  ],
  "dependencies": {
    "jsbi": "^3.1.4"
  }
}
